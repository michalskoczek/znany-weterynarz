{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true, "parser": "flow"}],
    "no-unused-vars": "warn",
    "new-cap": "off"
  }
}
